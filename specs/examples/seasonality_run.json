{
  "data": {
    "dataset_path": "specs/examples/data/eurusd_m1_sample.json",
    "symbols": ["EURUSD"],
    "timeframe": "M1",
    "start": "2024-01-01T00:00:00Z",
    "end": "2024-01-01T00:09:00Z"
  },
  "profile": {
    "by_hour": true,
    "by_dow": true,
    "measure": "direction",
    "ret_horizon": 1,
    "min_samples_bin": 2
  },
  "signal": {
    "method": "threshold",
    "threshold": 0.55,
    "dims": ["hour"],
    "combine": "and"
  },
  "compute": {
    "max_trials": 5,
    "search_space": {}
  },
  "execution": {
    "slippage_bps": 0.5,
    "commission_bps": 0.2
  },
  "risk": {
    "max_positions": 1,
    "max_allocation": 1.0
  },
  "tp_sl": {
    "stop_loss": 1.5,
    "take_profit": 1.5
  },
  "validation": {
    "train_months": 0,
    "test_months": 1,
    "folds": 1,
    "embargo_days": 0
  },
  "artifacts": {
    "out_dir": "runs/seasonality_demo"
  },
  "persistence": {
    "enabled": false
  }
}
